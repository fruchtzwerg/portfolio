    "astro": "^1.7.1",
    "astro-compress": "^1.1.23",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.45.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "12.2.3",
    "mime": "^3.0.0",
    "nx": "15.2.4",
    "postcss": "^8.4.20",
    "prettier": "2.8.0",
    "puppeteer": "^19.3.0",
    "rollup-plugin-visualizer": "^5.8.3",
