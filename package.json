{
  "name": "portfolio",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./tools/scripts/patch-nx-cli.mjs",
    "start": "nx serve",
    "mobile": "nx run portfolio:serve --configuration=mobile",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "nx:upgrade": "nx migrate latest",
    "nx:migrate": "nx migrate --run-migrations",
    "icons:extract": "nx extract icons && prettier --write libs/icons/",
    "scully": "yarn build --prod && scully --project portfolio --removeStaticDist",
    "scully:serve": "scully serve",
    "scully:scan": "yarn build && scully --project portfolio --scanRoutes",
    "scully:kill": "scully killServer"
  },
  "svgGenerator": {
    "outputPath": "libs/icons/src",
    "prefix": "app",
    "srcPath": "libs/icons/assets",
    "svgoConfig": {
      "plugins": [
        "removeDimensions"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@fontsource/arvo": "^5.0.8",
    "@fontsource/exo": "^5.0.8",
    "@fontsource/hind": "^5.0.8",
    "@fontsource/inconsolata": "^5.0.8",
    "@fontsource/lato": "^5.0.8",
    "@fontsource/quicksand": "^5.0.8",
    "svelte": "^4.2.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@astrojs/image": "^0.17.3",
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/prefetch": "^0.3.0",
    "@astrojs/sitemap": "^2.0.1",
    "@astrojs/svelte": "^3.1.0",
    "@astrojs/tailwind": "^4.0.0",
    "@iconify/json": "^2.2.102",
    "@iconify/tools": "^3.0.5",
    "@iconify/types": "^2.0.0",
    "@nx/devkit": "16.6.0",
    "@nx/eslint-plugin": "16.6.0",
    "@nx/jest": "16.6.0",
    "@nx/js": "16.6.0",
    "@nx/linter": "16.6.0",
    "@nx/plugin": "16.6.0",
    "@nx/workspace": "16.6.0",
    "@nxtensions/astro": "16.2.0",
    "@savvywombat/tailwindcss-grid-areas": "^3.1.0",
    "@swc-node/register": "~1.4.2",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.51",
    "@swc/helpers": "~0.5.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jest": "^29.4.0",
    "@types/node": "18.11.10",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@ungap/custom-elements": "^1.3.0",
    "astro": "^2.10.7",
    "astro-compress": "^2.0.8",
    "astro-robots-txt": "^0.5.0",
    "astro-seo": "^0.7.5",
    "autoprefixer": "^10.4.15",
    "cssnano": "^6.0.1",
    "daisyui": "^2.52.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-astro": "^0.21.1",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "mime": "^3.0.0",
    "nx": "16.6.0",
    "postcss": "^8.4.27",
    "prettier": "2.8.8",
    "prettier-plugin-astro": "^0.11.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.65.1",
    "sharp": "^0.32.4",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "unplugin-icons": "^0.15.3",
    "vite": "^4.4.9"
  }
}
