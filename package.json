{
  "name": "portfolio",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./tools/scripts/patch-nx-cli.mjs",
    "start": "nx serve",
    "mobile": "nx run portfolio:serve --configuration=mobile",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "nx:upgrade": "nx migrate latest",
    "nx:migrate": "nx migrate --run-migrations",
    "icons:extract": "nx extract icons && prettier --write libs/icons/",
    "scully": "yarn build --prod && scully --project portfolio --removeStaticDist",
    "scully:serve": "scully serve",
    "scully:scan": "yarn build && scully --project portfolio --scanRoutes",
    "scully:kill": "scully killServer"
  },
  "svgGenerator": {
    "outputPath": "libs/icons/src",
    "prefix": "app",
    "srcPath": "libs/icons/assets",
    "svgoConfig": {
      "plugins": [
        "removeDimensions"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@fontsource/arvo": "^4.5.7",
    "@fontsource/exo": "^4.5.11",
    "@fontsource/hind": "^4.5.10",
    "@fontsource/inconsolata": "^4.5.10",
    "@fontsource/lato": "^4.5.10",
    "@fontsource/quicksand": "^4.5.12",
    "svelte": "^3.59.1"
  },
  "devDependencies": {
    "@astrojs/image": "^0.17.0",
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/prefetch": "^0.1.2",
    "@astrojs/sitemap": "^1.3.3",
    "@astrojs/svelte": "^2.2.0",
    "@astrojs/tailwind": "^3.1.3",
    "@iconify/json": "^2.2.79",
    "@iconify/tools": "^2.2.6",
    "@iconify/types": "^2.0.0",
    "@nrwl/cli": "15.7.2",
    "@nrwl/devkit": "15.7.2",
    "@nrwl/eslint-plugin-nx": "15.7.2",
    "@nrwl/linter": "15.7.2",
    "@nrwl/workspace": "15.7.2",
    "@nxtensions/astro": "3.3.0",
    "@savvywombat/tailwindcss-grid-areas": "^3.1.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "18.11.10",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "@ungap/custom-elements": "^1.2.0",
    "astro": "^2.6.4",
    "astro-compress": "^1.1.47",
    "astro-robots-txt": "^0.5.0",
    "astro-seo": "^0.7.4",
    "autoprefixer": "^10.4.14",
    "cssnano": "^5.1.15",
    "daisyui": "^2.52.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-astro": "^0.21.1",
    "eslint-plugin-import": "^2.27.5",
    "mime": "^3.0.0",
    "nx": "15.7.2",
    "postcss": "^8.4.24",
    "prettier": "2.8.0",
    "prettier-plugin-astro": "^0.10.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.4",
    "sharp": "^0.31.3",
    "tailwindcss": "^3.3.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "unplugin-icons": "^0.15.3"
  }
}