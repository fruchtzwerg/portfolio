{
  "name": "portfolio",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "mobile": "nx run portfolio:serve --configuration=mobile",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "nx:upgrade": "nx migrate latest",
    "nx:migrate": "nx migrate --run-migrations",
    "icons:extract": "nx extract icons && prettier --write libs/icons/",
    "scully": "yarn build --prod && scully --project portfolio --removeStaticDist",
    "scully:serve": "scully serve",
    "scully:scan": "yarn build && scully --project portfolio --scanRoutes",
    "scully:kill": "scully killServer"
  },
  "svgGenerator": {
    "outputPath": "libs/icons/src",
    "prefix": "app",
    "srcPath": "libs/icons/assets",
    "svgoConfig": {
      "plugins": [
        "removeDimensions"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.2.1",
    "@angular/common": "13.2.1",
    "@angular/compiler": "13.2.1",
    "@angular/core": "13.2.1",
    "@angular/forms": "13.2.1",
    "@angular/platform-browser": "13.2.1",
    "@angular/platform-browser-dynamic": "13.2.1",
    "@angular/router": "13.2.1",
    "@fontsource/quicksand": "^4.5.2",
    "@fontsource/roboto": "^4.5.2",
    "@ngneat/svg-icon": "^4.0.2",
    "@nrwl/angular": "13.7.3",
    "@scullyio/ng-lib": "^2.1.11",
    "@scullyio/scully": "^2.1.11",
    "@scullyio/scully-plugin-flash-prevention": "^2.1.11",
    "@scullyio/scully-plugin-puppeteer": "^2.1.11",
    "primeicons": "^5.0.0",
    "primeng": "^13.1.0",
    "rxjs": "~7.5.2",
    "smoothscroll-anchor-polyfill": "^1.3.4",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "^2.0.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.2",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cdk": "13.2.1",
    "@angular/cli": "13.2.2",
    "@angular/compiler-cli": "13.2.1",
    "@angular/language-service": "13.2.1",
    "@material/elevation": "^13.0.0",
    "@ngbasics/eslint-plugin": "^13.0.3",
    "@ngneat/svg-generator": "^4.0.0",
    "@nrwl/cli": "13.7.3",
    "@nrwl/eslint-plugin-nx": "13.7.3",
    "@nrwl/jest": "13.7.3",
    "@nrwl/linter": "13.7.3",
    "@nrwl/tao": "13.7.3",
    "@nrwl/workspace": "13.7.3",
    "@scullyio/init": "^2.1.11",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "playwright": "^1.18.0",
    "prettier": "2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "4.5.5"
  }
}
