{
  "name": "portfolio",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.mjs && ngcc --properties es2020 browser module main && node ./tools/scripts/patch-nx-cli.mjs",
    "start": "nx serve",
    "mobile": "nx run portfolio:serve --configuration=mobile",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "nx:upgrade": "nx migrate latest",
    "nx:migrate": "nx migrate --run-migrations",
    "icons:extract": "nx extract icons && prettier --write libs/icons/",
    "scully": "yarn build --prod && scully --project portfolio --removeStaticDist",
    "scully:serve": "scully serve",
    "scully:scan": "yarn build && scully --project portfolio --scanRoutes",
    "scully:kill": "scully killServer"
  },
  "svgGenerator": {
    "outputPath": "libs/icons/src",
    "prefix": "app",
    "srcPath": "libs/icons/assets",
    "svgoConfig": {
      "plugins": [
        "removeDimensions"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.0.1",
    "@angular/common": "15.0.1",
    "@angular/compiler": "15.0.1",
    "@angular/core": "15.0.1",
    "@angular/forms": "15.0.1",
    "@angular/platform-browser": "15.0.1",
    "@angular/platform-browser-dynamic": "15.0.1",
    "@angular/router": "15.0.1",
    "@astrojs/image": "^0.11.6",
    "@astrojs/mdx": "^0.13.0",
    "@fontsource/quicksand": "^4.5.11",
    "@fontsource/roboto": "^4.5.8",
    "@ngneat/svg-icon": "^4.0.2",
    "@scullyio/ng-lib": "^2.1.41",
    "@scullyio/scully": "^2.1.41",
    "@scullyio/scully-plugin-flash-prevention": "^2.1.11",
    "@scullyio/scully-plugin-puppeteer": "^2.1.11",
    "primeicons": "^5.0.0",
    "primeng": "^14.2.2",
    "rxjs": "~7.5.0",
    "smoothscroll-anchor-polyfill": "^1.3.4",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "^2.0.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.1",
    "@angular-eslint/eslint-plugin": "14.0.4",
    "@angular-eslint/eslint-plugin-template": "14.0.4",
    "@angular-eslint/template-parser": "14.0.4",
    "@angular/cdk": "15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "15.0.1",
    "@angular/language-service": "15.0.1",
    "@astrojs/tailwind": "^2.1.3",
    "@iconify/json": "^2.1.153",
    "@iconify/tools": "^2.1.2",
    "@iconify/types": "^2.0.0",
    "@material/elevation": "^13.0.0",
    "@ngbasics/eslint-plugin": "^13.0.5",
    "@ngneat/svg-generator": "^4.0.0",
    "@nrwl/angular": "15.2.4",
    "@nrwl/cli": "15.2.4",
    "@nrwl/devkit": "15.2.4",
    "@nrwl/eslint-plugin-nx": "15.2.4",
    "@nrwl/jest": "15.2.4",
    "@nrwl/linter": "15.2.4",
    "@nrwl/workspace": "15.2.4",
    "@nxext/solid": "14.0.2",
    "@nxtensions/astro": "^3.1.0",
    "@savvywombat/tailwindcss-grid-areas": "^3.0.0",
